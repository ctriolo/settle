  |  patternUnits="userSpaceOnUse"
  |  width="#{IMAGE.width}"
  |  height="#{IMAGE.height}"
  |  x="0"
  |  y="0"
  |  spreadMethod = "repeat">
  |  <image
  |    xlink:href="#{IMAGE.href}"
